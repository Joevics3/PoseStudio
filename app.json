{
  "expo": {
    "name": "Pose Studio",
    "slug": "posestudio",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "posestudio",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.posestudio.app",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.posestudio.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "permissions": [
        "INTERNET",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-file-system",
        {
          "storagePermission": "Allow Pose Studio to save images to your device."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Pose Studio to save images to your photo library.",
          "savePhotosPermission": "Allow Pose Studio to save images to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a7b63695-c604-423a-b385-2005928e89fd"
      }
    },
    "owner": "joevicsapps"
  }
}
